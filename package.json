{
  "name": "@leetcode-hire/foundations-zh",
  "version": "1.0.0",
  "description": "Frontend foundation problem for leetcode-cn.com",
  "main": "lib/index.js",
  "repository": "https://github.com/LeetCode-OpenSource/hire",
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "scripts": {
    "lint:fix": "eslint . --ext .ts --fix",
    "lint": "eslint . --ext .ts",
    "compile": "tsc src/index.ts --outDir lib",
    "test": "jest",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,css,scss}\""
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npm run lint",
      "git add"
    ],
    "*.ts": "eslint --cache --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ts-node": "^10.9.1"
  }
}
